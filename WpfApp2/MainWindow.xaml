  Foreground="#DDFFFFFF"
  Padding="32"  UniformCornerRadius="0" Margin="0,72,987,0" Height="510" Grid.RowSpan="2" Grid.ColumnSpan="2"/>

        <materialDesign:Chip
  Content="Hello Metin&#xD;&#xA;"
