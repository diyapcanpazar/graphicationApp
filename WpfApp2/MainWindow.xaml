        <Label x:Name="label1_Copy1" Content="" HorizontalAlignment="Left" Height="64" Margin="890,117,0,0" VerticalAlignment="Top" Width="93" Grid.Column="1">
            <Label.Background>
                <ImageBrush ImageSource="dollar.png" Stretch="Uniform"/>
            </Label.Background>
        </Label>
        <materialDesign:Card
    materialDesign:ShadowAssist.ShadowDepth="Depth1"
    Padding="32" Margin="50,240,678,0" Height="297" Content="" Grid.RowSpan="2" Grid.Column="1"/>

        <Wpf:PieChart LegendLocation="Bottom" DataClick="Chart_OnDataClick" Hoverable="False" DataTooltip="{x:Null}" Margin="50,240,678,99" Grid.RowSpan="2" Loaded="PieChart_Loaded" Grid.Column="1">
            <Wpf:PieChart.Series>
                <Wpf:PieSeries Title="Ventas" Values="25" DataLabels="True"
                               LabelPoint="{Binding PointLabel}"/>
                <Wpf:PieSeries Title="Compras" Values="25" DataLabels="True" 
                               LabelPoint="{Binding PointLabel}"/>
                <Wpf:PieSeries Title="Produccion" Values="25" DataLabels="True" 
                               LabelPoint="{Binding PointLabel}"/>
                <Wpf:PieSeries Title="Analisis" Values="25" DataLabels="True" 
                               LabelPoint="{Binding PointLabel}"/>
            </Wpf:PieChart.Series>


        </Wpf:PieChart>

        <ToggleButton
  Style="{StaticResource MaterialDesignActionToggleButton}"
  ToolTip="MaterialDesignActionLightToggleButton" Margin="66,226,921,53" Background="#FF2195F2" Grid.Row="1" Grid.Column="1">
            <materialDesign:ToggleButtonAssist.OnContent>
                <materialDesign:PackIcon
      Kind="Sale" />
            </materialDesign:ToggleButtonAssist.OnContent>
            <materialDesign:PackIcon
                Kind="Sale" />
        </ToggleButton>
        <Wpf:CartesianChart Series="{Binding SeriesCollection}" LegendLocation="Right" Margin="366,253,19,99" Grid.RowSpan="2" Grid.Column="1" >
            <Wpf:CartesianChart.AxisY>
                <Wpf:Axis Title="Ventas del Mes" LabelFormatter="{Binding YFormatter}"/>
            </Wpf:CartesianChart.AxisY>
            <Wpf:CartesianChart.AxisX>
                <Wpf:Axis Title="Meses" Labels="{Binding Labels}"/>
            </Wpf:CartesianChart.AxisX>
        </Wpf:CartesianChart>
        <ToggleButton
            Style="{StaticResource MaterialDesignActionToggleButton}"
            ToolTip="MaterialDesignActionLightToggleButton" Margin="134,226,853,53" Background="#FFF34336" Grid.Row="1" Grid.Column="1">
            <materialDesign:ToggleButtonAssist.OnContent>
                <materialDesign:PackIcon
                    Kind="Shopping" />
            </materialDesign:ToggleButtonAssist.OnContent>
            <materialDesign:PackIcon
                Kind="Shopping" />
