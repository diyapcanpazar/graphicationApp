        <ToggleButton
            Style="{StaticResource MaterialDesignActionToggleButton}"
            ToolTip="MaterialDesignActionLightToggleButton" Margin="601,226,386,53" Background="#FFF34336" Grid.Row="1" Grid.Column="1">
            <materialDesign:ToggleButtonAssist.OnContent>
                <materialDesign:PackIcon
                    Kind="Shopping" />
            </materialDesign:ToggleButtonAssist.OnContent>
            <materialDesign:PackIcon
                Kind="Shopping" />
        </ToggleButton>
        <ToggleButton
            Style="{StaticResource MaterialDesignActionToggleButton}"
            ToolTip="MaterialDesignActionLightToggleButton" Margin="724,226,263,53" Background="#FFFEC007" Grid.Row="1" Grid.Column="1">
            <materialDesign:ToggleButtonAssist.OnContent>
                <materialDesign:PackIcon
                    Kind="Reproduction" />
            </materialDesign:ToggleButtonAssist.OnContent>
