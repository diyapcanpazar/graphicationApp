            <materialDesign:ToggleButtonAssist.OnContent>
                <materialDesign:PackIcon
                    Kind="Check" />
            </materialDesign:ToggleButtonAssist.OnContent>
            <materialDesign:PackIcon
                Kind="Check" />
        </ToggleButton>
        <Image Margin="8,210,999,23" Source="1550062568995.png" Stretch="Fill" RenderTransformOrigin="0.115,0.551" Grid.Row="1" Grid.ColumnSpan="2" RenderOptions.BitmapScalingMode="HighQuality" RenderOptions.EdgeMode="Aliased"/>
        <ProgressBar x:Name="Pb1" HorizontalAlignment="Left" Height="10" VerticalAlignment="Top" Width="190" Grid.Column="1" Margin="134,151,0,0" Background="#FF0778FF" BorderBrush="#FF9DDB9D" Maximum="100" Minimum="0" />
    </Grid>

</Window>
