        <Label x:Name="label1_Copy" Content="" HorizontalAlignment="Left" Height="64" Margin="562,119,0,0" VerticalAlignment="Top" Width="92" Grid.Column="1">
            <Label.Background>
                <ImageBrush ImageSource="tags.png" Stretch="Uniform"/>
            </Label.Background>
        </Label>
        <materialDesign:RatingBar
            Value="5"
            x:Name="BasicRatingBar_Copy1" Margin="870,195,19,45" Foreground="White" Background="Transparent" Grid.Column="1"/>
        <Label x:Name="label_Copy5" Content="Ventas En Dolar" HorizontalAlignment="Left" Height="28" Margin="724,115,0,0" VerticalAlignment="Top" Width="148" FontSize="14" Foreground="#DDF9F9F9" Grid.Column="1"/>
