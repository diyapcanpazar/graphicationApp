        mc:Ignorable="d"
        Title="MainWindow" Height="582" Width="1085.3" WindowStartupLocation="CenterScreen" ShowInTaskbar="False" AllowsTransparency="True" WindowStyle="None">
    <Grid SnapsToDevicePixels="True">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="66*"/>
            <ColumnDefinition Width="1019*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="271*"/>
            <RowDefinition Height="311*"/>
        </Grid.RowDefinitions>
        <materialDesign:Card
    materialDesign:ShadowAssist.ShadowDepth="Depth1"
    Padding="32" HorizontalAlignment="Right" Width="634" Margin="0,253,19,0" Height="284" Content="" Grid.RowSpan="2" Grid.Column="1"/>

        <materialDesign:ColorZone
  Mode="PrimaryMid"
  Padding="16" Background="#104e9f" Grid.ColumnSpan="2">
            <DockPanel>
                <StackPanel
      Orientation="Horizontal"
      materialDesign:RippleAssist.IsCentered="True">
                    <ToggleButton
        Style="{DynamicResource MaterialDesignHamburgerToggleButton}" Width="61" />
                    <Button
        Style="{DynamicResource MaterialDesignToolForegroundButton}"
        Margin="8 0 0 0"
        Panel.ZIndex="0"/>
                </StackPanel>
            </DockPanel>
        </materialDesign:ColorZone>
        <ToggleButton
  Style="{StaticResource MaterialDesignFlatPrimaryToggleButton}"
  ToolTip="MaterialDesignFlatPrimaryToggleButton"
  IsChecked="False" Margin="768,21,221,222" Height="Auto" Width="Auto" Foreground="#FF56618D" Grid.Column="1">
            <materialDesign:PackIcon
    Kind="NotificationsActive"
    Height="21"
    Width="21" Foreground="White" />
        </ToggleButton>

        <Separator HorizontalAlignment="Left" Height="16" Margin="808,28,0,0" VerticalAlignment="Top" Width="24" RenderTransformOrigin="0.5,0.5" Grid.Column="1">
            <Separator.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Separator.RenderTransform>
        </Separator>
        <materialDesign:Card
