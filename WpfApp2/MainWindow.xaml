        <ToggleButton
            Style="{StaticResource MaterialDesignActionToggleButton}"
            ToolTip="MaterialDesignActionLightToggleButton" Margin="212,226,775,53" Background="#FFFEC007" Grid.Row="1" Grid.Column="1">
            <materialDesign:ToggleButtonAssist.OnContent>
                <materialDesign:PackIcon
                    Kind="Reproduction" />
